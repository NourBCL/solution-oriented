<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="38.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CommandeController">
   <children>
      <Label fx:id="od" alignment="CENTER" prefHeight="21.0" prefWidth="78.0" text="id" textAlignment="CENTER">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Label>
      <Label fx:id="ad" prefHeight="21.0" prefWidth="206.0" text="address">
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </Label>
      <Label fx:id="dt" alignment="CENTER" minWidth="-Infinity" prefHeight="21.0" prefWidth="173.0" text="date" textAlignment="CENTER">
         <padding>
            <Insets left="40.0" right="40.0" />
         </padding>
      </Label>
      <Button fx:id="sh" mnemonicParsing="false" onAction="#afficher" prefHeight="31.0" prefWidth="96.0" text="show">
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="ed" mnemonicParsing="false" onAction="#modifier" prefHeight="31.0" prefWidth="100.0" text="edit">
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="de" mnemonicParsing="false" onAction="#supprimer" prefHeight="31.0" prefWidth="110.0" text="delete">
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
