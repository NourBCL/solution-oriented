

{% block title %}CommandeT index{% endblock %}

{% block body %}

    <h1>CommandeT index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Date_creation</th>
                <th>Address_destination</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for commande_t in commande_ts %}
            <tr>
                <td>{{ commande_t.id }}</td>
                <td>{{ commande_t.dateCreation ? commande_t.dateCreation|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ commande_t.addressDestination }}</td>
                <td>
                    <a href="{{ path('commande_t_show', {'id': commande_t.id}) }}">show</a>
                    <a href="{{ path('commande_t_edit', {'id': commande_t.id}) }}">edit</a>
                    <a href="{{ path('commande_t_delete', {'id': commande_t.id}) }}">delete</a>

                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('commande_t_new_admin') }}">Create new</a>
{% endblock %}
