
{% block title %}Modifier Commande Evenement{% endblock %}

{% block body %}
<div class="content-body">
    <div class="container-fluid">
        <div class="page-titles">
            <h4>Commande Evenement</h4>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                <li class="breadcrumb-item active"><a href="javascript:void(0)">Modifier Commande Evenement</a></li>
            </ol>
        </div>
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header">
                </div>
                <div class="card-body">
                    <div class="basic-form">
                        {{ form_start(form,{'attr': {'novalidate': 'novalidate'}} ) }}
                        <div id="wizard_Service" class="tab-pane" role="tabpanel">
                            <div class="row">
                                <div class="col-lg-6 mb-2">
                                    <div class="form-group">
                                        <div class="form-group">
                                            <label class="text-label"></label>
                                            {{ form_widget(form.date_creation,{'attr':{ 'class':'form-control input-default' ,'placeholder':'Nom'}}) }}
                                            {{ form_errors(form.nomCat_e) }}
                        {{ include('commande_e/_form.html.twig', {'button_label': 'Update'}) }}

    <a href="{{ path('commande_e_index') }}">back to list</a>

    {{ include('commande_e/_delete_form.html.twig') }}
