<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="cardItem" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Interfaces.RegionItemController">
   <children>
      <ImageView fx:id="imgJeu" fitHeight="212.0" fitWidth="455.0" pickOnBounds="true" />
      <ImageView fx:id="add_jeu_image" fitHeight="170.0" fitWidth="455.0" onMousePressed="#add_jeu_image_pressed" pickOnBounds="true" preserveRatio="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="edit_jeu" fitHeight="32.0" fitWidth="39.0" layoutX="17.0" layoutY="176.0" onMousePressed="#edit_jeu_pressed" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/note-pencil-bold.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="remove_jeu" fitHeight="32.0" fitWidth="39.0" layoutX="56.0" layoutY="176.0" onMousePressed="#remove_jeu_pressed" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/trash-bold.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Label fx:id="regionnamer" layoutX="4.0" layoutY="8.0" prefHeight="37.0" prefWidth="217.0" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
